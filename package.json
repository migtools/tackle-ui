{
  "name": "tackle-ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@konveyor/lib-ui": "^5.2.0",
    "@patternfly/patternfly": "4.164.2",
    "@patternfly/react-charts": "6.34.1",
    "@patternfly/react-core": "4.181.1",
    "@patternfly/react-table": "4.50.1",
    "@patternfly/react-tokens": "4.11.2",
    "@react-keycloak/web": "^3.4.0",
    "@redhat-cloud-services/frontend-components-notifications": "3.2.2",
    "axios": "^0.21.1",
    "file-saver": "^2.0.5",
    "formik": "^2.2.6",
    "i18next": "^19.8.4",
    "i18next-http-backend": "^1.0.22",
    "keycloak-js": "12.0.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-i18next": "^11.8.5",
    "react-measure": "^2.5.2",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "5.0.0",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "typesafe-actions": "^5.1.0",
    "web-vitals": "^0.2.4",
    "yup": "^0.32.8"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "extract": "i18next --config i18next-parser.config.js",
    "start": "react-scripts -r @cypress/instrument-cra start",
    "build": "react-scripts build",
    "build:dev": "GENERATE_SOURCEMAP=true react-scripts build",
    "build:instrumentation": "CYPRESS_INSTRUMENT_PRODUCTION=true react-scripts -r @cypress/instrument-cra build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "ui:start": "node server.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.9.11",
    "@cypress/instrument-cra": "^1.4.0",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^4.0.1",
    "@testing-library/user-event": "^13.5.0",
    "@types/enzyme": "^3.10.11",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/file-saver": "^2.0.2",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-measure": "^2.0.6",
    "@types/react-redux": "^7.1.15",
    "@types/react-router-dom": "^5.1.7",
    "@types/redux-logger": "^3.0.8",
    "@types/yup": "^0.29.11",
    "axios-mock-adapter": "^1.19.0",
    "cypress": "8.4.0",
    "cypress-file-upload": "^5.0.8",
    "cypress-keycloak-commands": "^1.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "http-proxy-middleware": "^1.0.6",
    "husky": "^4.3.7",
    "i18next-parser": "^3.6.0",
    "jest-enzyme": "^7.1.2",
    "lint-staged": "^10.5.3",
    "node-sass": "^4.14.1",
    "prettier": "^2.2.1",
    "redux-devtools-extension": "^2.13.8",
    "source-map-explorer": "^2.5.2",
    "typescript": "4.0.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/",
      "!src/**/*.stories.*"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-commit": "npm run extract"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "nyc": {
    "exclude": "src/reportWebVitals.ts"
  }
}
